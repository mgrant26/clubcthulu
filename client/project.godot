config_version=5

[application]

config/name="Club Cthulhu"
run/main_scene="res://Scenes/DirectConnect.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=1000
config/icon="res://icon.svg"

[autoload]

ConnectionHandler="*res://Connection/ConnectionHandler.gd"
WebSocketConnectionHandler="*res://Connection/WebSocketConnectionHandler.gd"

[display]

window/stretch/mode="viewport"

[editor]

